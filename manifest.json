{
  "manifest_version": 2,
  "name": "Darken",
  "version": "1.0.0",
  "description": "Darken YouTube videos you've already watched.",
  "icons": {
    "16": "assets/darken_16.png",
    "48": "assets/darken_48.png",
    "128": "assets/darken_128.png"
  },
  "author": "Peter Han",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["styles.css"]
    }
  ]
}
